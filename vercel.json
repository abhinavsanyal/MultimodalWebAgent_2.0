{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "dist"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/src/oai_agent/utils/prompt"
      }
    ],
    "env": {
      "OPENAI_API_KEY": "@openai_api_key"
    }
  }
  