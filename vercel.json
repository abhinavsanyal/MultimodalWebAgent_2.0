{
    "version": 2,
    "builds": [
      {
        "src": "requirements.txt",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/src/oai_agent/utils/prompt"
      }
    ],
    "env": {
      "OPENAI_API_KEY": "@openai_api_key"
    }
  }
  